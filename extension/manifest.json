{
    "manifest_version": 3,
    "name": "YouTubeGPT",
    "description": "YouTubeGPT Summarizer",
    "version": "0.1",
    "permissions": ["tabs","scripting"],
    "content_scripts": [{"matches": ["https://www.youtube.com/*","http://www.youtube.com/*"], "js": ["main.js"]}],
    "content_security_policy": {"extension_pages": "script-src 'self'; object-src 'self'"}
}